
#include <iostream>
#include <algorithm>
using namespace std;

int main()
{
    int n;
    cin >> n
    while ( n!=0 ) 
    {
        int count=0;
        for (i = n; i < 2n; i++) 
        {
            if (is_sosuu(i) = 1) 
            {
                count++
            } 
        }
        
        cout << count << endl;
        cin >> n
    }
    return 0;
}
